generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model shli_constant {
  id             Int       @id @default(autoincrement())
  date_updated   DateTime  @default(now()) @db.Timestamp(6)
  date_created   DateTime  @default(now()) @db.Timestamp(6)
  date_published DateTime? @db.Timestamp(6)
  published      Int       @default(1) @db.SmallInt
  deleted        Int       @default(0) @db.SmallInt
  manorder       Int       @default(autoincrement())
  ident          String    @default("") @db.VarChar(250)
  hashkey        String    @default("") @db.VarChar(250)
  content        String?
}

model shli_m2m_room_person {
  id             Int         @id @default(autoincrement())
  date_updated   DateTime    @default(now()) @db.Timestamp(6)
  date_created   DateTime    @default(now()) @db.Timestamp(6)
  date_published DateTime?   @db.Timestamp(6)
  published      Boolean     @default(true)
  deleted        Boolean     @default(false)
  manorder       Int         @default(autoincrement())
  ident          String      @default("") @db.VarChar(250)
  room           Int         @default(0)
  person         Int         @default(0)
  color          String      @default("") @db.VarChar(16)
  can_invite     Boolean     @default(false)
  can_edit       Boolean     @default(false)
  Person         shli_person @relation(fields: [person], references: [id], onDelete: NoAction, onUpdate: NoAction)
  Room           shli_room   @relation(fields: [room], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model shli_message {
  id             Int            @id @default(autoincrement())
  date_updated   DateTime       @default(now()) @db.Timestamp(6)
  date_created   DateTime       @default(now()) @db.Timestamp(6)
  date_published DateTime?      @db.Timestamp(6)
  published      Boolean        @default(true)
  deleted        Boolean        @default(false)
  manorder       Int            @default(autoincrement())
  ident          String         @default("") @db.VarChar(250)
  replyto_id     Int?
  forwardfrom_id Int?
  room           Int
  person         Int
  persons_sent   Int[]
  persons_read   Int[]
  content        String?
  archived       Boolean        @default(false)
  edited         Boolean        @default(false)
  purchase       Int?
  ForwardFrom    shli_message?  @relation("shli_messageToshli_message_forwardfrom_id", fields: [forwardfrom_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  Creator        shli_person    @relation(fields: [person], references: [id], onDelete: NoAction, onUpdate: NoAction)
  Purchase       shli_purchase? @relation(fields: [purchase], references: [id], onDelete: NoAction, onUpdate: NoAction)
  ReplyTo        shli_message?  @relation("shli_messageToshli_message_replyto_id", fields: [replyto_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  Room           shli_room      @relation(fields: [room], references: [id], onDelete: NoAction, onUpdate: NoAction)
  ForwardedAs    shli_message[] @relation("shli_messageToshli_message_forwardfrom_id")
  Replies        shli_message[] @relation("shli_messageToshli_message_replyto_id")
}

