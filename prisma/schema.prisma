model shli_m2m_puritem_person {
  id             Int       @id @default(autoincrement())
  date_updated   DateTime  @default(now()) @db.Timestamp(6)
  date_created   DateTime  @default(now()) @db.Timestamp(6)
  date_published DateTime? @db.Timestamp(6)
  published      Int       @default(1) @db.SmallInt
  deleted        Int       @default(0) @db.SmallInt
  manorder       Int       @default(autoincrement())
  ident          String?   @default("") @db.VarChar(250)
  puritem        Int       @default(0)
  room           Int       @default(0)
  purchase       Int       @default(0)
  person_bought  Int       @default(0)
  product        Int       @default(0)
  price          Decimal?  @db.Decimal(7, 2)
  qnty           Decimal?  @db.Decimal(7, 2)
  comment        String?
}

model shli_m2m_room_person {
  id             Int         @id @default(autoincrement())
  date_updated   DateTime    @default(now()) @db.Timestamp(6)
  date_created   DateTime    @default(now()) @db.Timestamp(6)
  date_published DateTime?   @db.Timestamp(6)
  published      Int         @default(1) @db.SmallInt
  deleted        Int         @default(0) @db.SmallInt
  manorder       Int         @default(autoincrement())
  ident          String?     @default("") @db.VarChar(250)
  room           Int         @default(0)
  person         Int         @default(0)
  shli_person    shli_person @relation(fields: [person], references: [id], onDelete: NoAction, onUpdate: NoAction)
  shli_room      shli_room   @relation(fields: [room], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model shli_message {
  id             Int       @id @default(autoincrement())
  date_updated   DateTime  @default(now()) @db.Timestamp(6)
  date_created   DateTime  @default(now()) @db.Timestamp(6)
  date_published DateTime? @db.Timestamp(6)
  published      Int       @default(1) @db.SmallInt
  deleted        Int       @default(0) @db.SmallInt
  manorder       Int       @default(autoincrement())
  ident          String    @default("") @db.VarChar(250)
  room           Int
  person         Int
  content        String?
  edited         Boolean   @default(false) @db.Boolean
  purchase       Int?
  shli_person    shli_person    @relation(fields: [person], references: [id], onDelete: NoAction, onUpdate: NoAction)
  shli_purchase  shli_purchase? @relation(fields: [purchase], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model shli_person {
  id                                                        Int                    @id @default(autoincrement())
  date_updated                                              DateTime               @default(now()) @db.Timestamp(6)
  date_created                                              DateTime               @default(now()) @db.Timestamp(6)
  date_published                                            DateTime?              @db.Timestamp(6)
  published                                                 Int                    @default(1) @db.SmallInt
  deleted                                                   Int                    @default(0) @db.SmallInt
  manorder                                                  Int                    @default(autoincrement())
  ident                                                     String                 @default("") @db.VarChar(250)
  phone                                                     String                 @default("") @db.VarChar(250)
  email                                                     String                 @default("") @db.VarChar(250)
  password                                                  String                 @default("") @db.VarChar(250)
  auth                                                      String                 @default("") @db.VarChar(250)
  date_lastclick                                            DateTime?              @db.Timestamp(6)
  lastip                                                    String                 @default("") @db.VarChar(250)
  lastsid                                                   String                 @default("") @db.VarChar(250)
  shli_m2m_room_person                                      shli_m2m_room_person[]
  shli_purchase_shli_personToshli_purchase_person_created   shli_purchase[]        @relation("shli_personToshli_purchase_person_created")
  shli_purchase_shli_personToshli_purchase_person_purchased shli_purchase[]        @relation("shli_personToshli_purchase_person_purchased")
  shli_room                                                 shli_room[]
  Messages            shli_message[]
}

model shli_pgroup {
  id              Int            @id @default(autoincrement())
  date_updated    DateTime       @default(now()) @db.Timestamp(6)
  date_created    DateTime       @default(now()) @db.Timestamp(6)
  date_published  DateTime?      @db.Timestamp(6)
  published       Int            @default(1) @db.SmallInt
  deleted         Int            @default(0) @db.SmallInt
  manorder        Int            @default(autoincrement())
  ident           String         @default("") @db.VarChar(250)
  parent_id       Int
  room            Int
  purchase_origin Int
  shli_purchase   shli_purchase  @relation(fields: [purchase_origin], references: [id], onDelete: NoAction, onUpdate: NoAction)
  shli_room       shli_room      @relation(fields: [room], references: [id], onDelete: NoAction, onUpdate: NoAction)
  shli_product    shli_product[]
  shli_puritem    shli_puritem[]
}

model shli_product {
  id              Int            @id @default(autoincrement())
  date_updated    DateTime       @default(now()) @db.Timestamp(6)
  date_created    DateTime       @default(now()) @db.Timestamp(6)
  date_published  DateTime?      @db.Timestamp(6)
  published       Int            @default(1) @db.SmallInt
  deleted         Int            @default(0) @db.SmallInt
  manorder        Int            @default(autoincrement())
  ident           String         @default("") @db.VarChar(250)
  pgroup          Int?
  room            Int
  purchase_origin Int
  punit           Int?
  weight          Decimal        @default(0) @db.Decimal(7, 2)
  price_avg       Decimal        @default(0) @db.Decimal(7, 2)
  shli_pgroup     shli_pgroup?   @relation(fields: [pgroup], references: [id], onDelete: NoAction, onUpdate: NoAction)
  shli_purchase   shli_purchase  @relation(fields: [purchase_origin], references: [id], onDelete: NoAction, onUpdate: NoAction)
  shli_room       shli_room      @relation(fields: [room], references: [id], onDelete: NoAction, onUpdate: NoAction)
  shli_puritem    shli_puritem[]
}

model shli_punit {
  id             Int       @id @default(autoincrement())
  date_updated   DateTime  @default(now()) @db.Timestamp(6)
  date_created   DateTime  @default(now()) @db.Timestamp(6)
  date_published DateTime? @db.Timestamp(6)
  published      Int       @default(1) @db.SmallInt
  deleted        Int       @default(0) @db.SmallInt
  manorder       Int       @default(autoincrement())
  ident          String    @default("") @db.VarChar(250)
  brief          String    @default("") @db.VarChar(250)
  fpoint         Int       @default(0) @db.SmallInt
}

